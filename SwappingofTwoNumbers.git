
public class SwappingofTwoNumbers {
	
	public static void main(String args[])
	{
		int a=12, b=14;
		
		System.out.println("Values Before Swapping");
		System.out.println(a);
		System.out.println(b);
		
		a=a+b;
		b=a-b;
		a=a-b;
		
		System.out.println("Values After Swapping");
		System.out.println(a);
		System.out.println(b);
	}

}
